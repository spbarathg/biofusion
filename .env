# AntBot Environment Configuration

# Solana network settings
SOLANA_NETWORK_URL=https://api.mainnet-beta.solana.com
SOLANA_NETWORK_TYPE=mainnet-beta
RPC_URL=https://api.mainnet-beta.solana.com
WS_URL=wss://api.mainnet-beta.solana.com

# Dashboard settings
DASHBOARD_PORT=8501
DASHBOARD_HOST=0.0.0.0
DASHBOARD_PASSWORD=changeme

# Logging settings
LOG_LEVEL=INFO
LOG_DIR=logs
LOG_MAX_SIZE=50  # MB
LOG_RETENTION=30  # days

# Security settings
ENCRYPTION_KEY_PATH=/app/config/secrets/.encryption_key

# Performance settings
MAX_WORKERS=5
MAX_CONCURRENT_REQUESTS=50
REQUEST_TIMEOUT=30  # seconds
WORKER_ALLOCATION_PERCENT=20

# Wallet settings
DEFAULT_WALLET_TYPE=queen
DEFAULT_FEE_PAYER=queen_main
WALLET_DIR=data/wallets
BACKUP_DIR=data/backups

# Trading limits (in SOL)
MAX_TRANSACTION_SIZE=10
MIN_SOL_RESERVE=0.5
MIN_PROFIT_THRESHOLD=0.5
MAX_PRICE_IMPACT=1.0
MAX_PATH_LENGTH=3 